@model RuntimeLogPageQueryWhere
@using Monitor.Constant
<div class="row">
    <div class="col-sm-12" style="border-bottom: 1px solid #eee;background-color: #F3F3F3;padding:10px 0 10px 0;">
        <input type="hidden" asp-for="PageIndex" />
        <label class="ml-20">
            项目:
            <select asp-for="ProjectID" class="form-control"></select>
        </label>
        <label class="ml-20">
            服务器:
            <select asp-for="ServicerID" class="form-control"></select>
        </label>
        <label class="ml-20">
            日志级别:
            <select asp-for="LogLevel" class="form-control" asp-items="SelectItemUtil.ToListItems<LogLevel>()">
                <option value="">-请选择-</option>
            </select>
        </label>
        <label class="ml-20">
            日志来源:
            <select asp-for="Source" class="form-control" asp-items="SelectItemUtil.ToListItems<Source>()">
                <option value="">-请选择-</option>
            </select>
        </label>
        <label class="ml-20">RequestID:<input type="text" asp-for="RequestGuid" class="form-control input-sm"></label>
        <label class="ml-20">执行时间:</label>

        <input type="submit" class="form-control input-sm btn btn-info ml-15" value="查询" onclick="RestPageIndex()" />
    </div>
</div> 